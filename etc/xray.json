{
    "inbounds": [
        {
            "protocol": "vless",
            "settings": 
            {
                "clients": [{"id": "$AUUID","flow": "xtls-rprx-direct","level": 0}],
                "decryption": "none",
                "fallbacks": [
                    {"path":"/vless","dest":1234,"xver":1},
                    {"path":"/vmess","dest":2345,"xver":1}
                ]
            },
            "streamSettings": {"network": "tcp","security": "xtls",}
        },
        {
            "port": 1234,"listen": "/etc/caddy/vless","protocol": "vless",
            "settings": {"clients": [{"id": "$AUUID","level": 0,}],"decryption": "none"},
            "streamSettings": {"network": "ws","security": "none","wsSettings": {"path": "/vless"}}
        },
        {
            "port": 2345,"listen": "/etc/caddy/vmess","protocol": "vmess",
            "settings": {"clients": [{"id": "$AUUID","level": 0}]},
            "streamSettings": {"network": "ws","security": "none","wsSettings": {"path": "/vmess"}}
        }
    ],

    "outbounds": 
    [
        {"protocol": "freedom","tag": "direct","settings": {}},
        {"protocol": "blackhole","tag": "blocked","settings": {}}
    ],
    
    "routing": 
    {
        "rules": 
        [
            {"type": "field","outboundTag": "blocked","domain": ["geosite:category-ads-all"]}
        ]
    }
}
